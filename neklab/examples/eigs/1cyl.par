[GENERAL]
startFrom = 0
stopAt = numSteps
numSteps = 100

dt = 1.000000E-02
timeStepper = bdf2
variableDt = no
targetCFL = 1.0000

writeControl = runTime
writeInterval = 100.
writeDoublePrecision = no

dealiasing = yes
#filtering = explicit
#filterWeight = 0.01
#filterCutoffRatio = 0.84

[PRESSURE]
preconditioner = semg_xxt
residualTol = 1.0E-9
residualProj = no
writeToFieldFile = yes

[VELOCITY]
residualTol = 1.0E-9
residualProj = no
density = 1.0
viscosity = -100.0
